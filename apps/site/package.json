{
  "name": "katha",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "node --run build:default -- --turbo",
    "build:default": "cross-env NODE_NO_WARNINGS=1 next build",
    "build:turbopack": "cross-env NODE_NO_WARNINGS=1 next build --turbopack",
    "dev": "cross-env NODE_NO_WARNINGS=1 next dev --turbo",
    "dev:turbopack": "cross-env NODE_NO_WARNINGS=1 next dev --turbopack",
    "lint": "turbo run lint:js lint:css",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint:fix": "node --run lint -- -- --fix",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\"",
    "start": "cross-env NODE_NO_WARNINGS=1 next start"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "cross-env": "^7.0.3",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scan": "^0.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.35.0"
  }
}
